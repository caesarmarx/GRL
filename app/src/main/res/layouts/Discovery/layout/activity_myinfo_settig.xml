<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/list_group_color">
    <include layout="@layout/layout_header"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">
       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="vertical"
           android:background="@color/white">
           <include layout="@layout/normal_section_row" />
           <RelativeLayout
               android:id="@+id/layoutPhoto"
               android:layout_width="match_parent"
               android:layout_height="80dp"
               android:paddingLeft="@dimen/row_view_margin"
               android:paddingRight="@dimen/row_view_margin"
               android:background="@drawable/setting_item_selector"
               android:paddingTop="5dp"
               android:paddingBottom="5dp">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:textSize="@dimen/font17"
                   android:text="@string/user_photo_titleq"/>
               <com.makeramen.roundedimageview.RoundedImageView
                   android:id="@+id/imgUserPhoto"
                   android:layout_width="70dp"
                   android:layout_height="70dp"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:scaleType="fitXY"
                   android:src="@drawable/user_default"
                   app:riv_corner_radius="@dimen/normal_radius"
                   app:riv_oval="false"/>

               <TextView
                   android:id="@+id/txtUserId"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_alignParentBottom="true"
                   android:textColor="@color/light_gray_color"
                   android:textSize="@dimen/font15"
                   android:text=""/>
           </RelativeLayout>
           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserName"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_name_title"/>
               <TextView
                   android:id="@+id/txtUserName"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserNick"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_nick_title"/>
               <TextView
                   android:id="@+id/txtUserNick"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>
           <RelativeLayout
               android:id="@+id/layoutUserCert"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_cert_title"/>
               <TextView
                   android:id="@+id/txtCertAmount"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"/>
           </RelativeLayout>
           <include layout="@layout/normal_section_row" />

           <TextView
               android:id="@+id/tvPrice"
               style="@style/TxtTitle"
               android:layout_height="@dimen/samll_cell_height"
               android:textSize="@dimen/font17"
               android:gravity="center_vertical"
               android:background="@drawable/setting_item_selector"
               android:layout_width="fill_parent"
               android:paddingLeft="@dimen/row_view_margin"
               android:paddingRight="@dimen/row_view_margin"
               android:layout_gravity="center_vertical"
               android:drawablePadding="@dimen/border_margin"
               android:drawableRight="@drawable/right"
               android:text="@string/user_price_title" />
           <include layout="@layout/normal_section_row" />

           <RelativeLayout
               android:id="@+id/layoutUserGender"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_gender_title"/>
               <TextView
                   android:id="@+id/txtUserGender"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserArea"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_area_title"/>
               <TextView
                   android:id="@+id/txtUserArea"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserSkill"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_skill_title"/>
               <TextView
                   android:id="@+id/txtUserSkill"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserJob"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_job_title"/>
               <TextView
                   android:id="@+id/txtUserJob"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/normal_section_row"/>

           <RelativeLayout
               android:id="@+id/layoutUserEnergy"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_energy_title"/>
               <TextView
                   android:id="@+id/txtUserEnergy"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>
           <RelativeLayout
               android:id="@+id/layoutUserDonate"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_donate_title"/>
               <TextView
                   android:id="@+id/txtUserDonate"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserEst"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_est_title"/>
               <TextView
                   android:id="@+id/txtUserEst"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserComplete"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_complete_title"/>
               <TextView
                   android:id="@+id/txtUserComplet"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <include layout="@layout/layout_border"
               android:layout_width="match_parent"
               android:layout_height="1px"
               android:layout_marginLeft="@dimen/border_margin"/>

           <RelativeLayout
               android:id="@+id/layoutUserAccount"
               android:layout_width="match_parent"
               android:layout_height="@dimen/samll_cell_height"
               android:background="@drawable/setting_item_selector">
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentLeft="true"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@android:color/black"
                   android:text="@string/user_account_title"/>
               <TextView
                   android:id="@+id/txtUserAccount"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_alignParentRight="true"
                   android:layout_centerVertical="true"
                   android:layout_marginRight="@dimen/row_view_margin"
                   android:textSize="@dimen/font17"
                   android:textColor="@color/light_gray_color"
                   android:text=""/>
           </RelativeLayout>

           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_height="@dimen/list_section_height">

               <View
                   android:layout_width="match_parent"
                   android:layout_height="match_parent"
                   android:background="@color/list_group_color"/>
               <include layout="@layout/layout_border"
                   android:layout_width="match_parent"
                   android:layout_height="1px"
                   android:layout_alignParentTop="true"/>
           </RelativeLayout>
       </LinearLayout>
    </ScrollView>
</LinearLayout>